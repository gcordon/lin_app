/* #components-layout-demo-fixed-sider  */
.logo {
    height: 32px;
    background: rgba(255, 255, 255, .2);
    margin: 16px;
    text-align: center;
    line-height: 32px;
}

.logo::before {
    content: 'EGG 后台';
    font-weight: bold;
    color: #fff;
}

.logo>img {
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
}
